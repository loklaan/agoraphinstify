  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/loklaan/agoraphinstify/issues"
  },
  "homepage": "https://github.com/loklaan/agoraphinstify",
  "dependencies": {
    "bower": "~1.8.0",
    "errorhandler": "~1.5.0",
    "express": "~4.15.2",
    "grunt": "~1.0.1",
    "grunt-cli": "~1.2.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.0.0",
    "grunt-contrib-uglify": "^2.2.1",
    "grunt-usemin": "^3.1.1",
    "grunt-wiredep": "~3.0.1",
