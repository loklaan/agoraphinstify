    "grunt-contrib-jshint": "~1.1.0",
    "grunt-jasmine-node-coverage": "~1.1.1",
    "grunt-karma": "~2.0.0",
    "karma": "~1.5.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-firefox-launcher": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "protractor": "~5.1.1",
    "supertest": "~3.0.0"
