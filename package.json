{
  "name": "agoraphinstify",
  "version": "0.3.6",
  "description": "Server for Agoraphinstify. And dev flow.",
  "scripts": {
    "start": "node server.js",
    "postinstall": "node node_modules/bower/bin/bower install && [ '$NODE_ENV' != 'production' ] && node node_modules/grunt-cli/bin/grunt setup",
    "test": "node node_modules/grunt-cli/bin/grunt test",
    "pretest-e2e": "node node_modules/protractor/bin/webdriver-manager update",
    "test-e2e": "node node_modules/protractor/bin/protractor tests/e2e.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loklaan/agoraphinstify.git"
